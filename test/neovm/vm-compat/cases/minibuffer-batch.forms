; minibuffer behavior in batch mode should signal end-of-file for interactive reads.
(minibufferp)
(minibuffer-depth)
(condition-case err (read-from-minibuffer "") (error (car err)))
(condition-case err (read-string "") (error (car err)))
(condition-case err (completing-read "" '("a" "b")) (error (car err)))
(condition-case err (read-from-minibuffer "" nil nil nil nil nil nil nil) (error (car err)))
(condition-case err (read-string "" nil nil nil nil nil) (error (car err)))
(condition-case err (read-number "" 1 nil nil) (error (car err)))
(condition-case err (completing-read "" '("a" "b") nil nil nil nil nil nil nil) (error (car err)))
(condition-case err (read-number 123) (error (car err)))
(condition-case err (read-number nil) (error (car err)))
(try-completion "" '("a") nil)
(all-completions "" '("a") nil nil)
(test-completion "" '("a") nil)
(condition-case err (try-completion "" '("a") nil nil) (error (car err)))
(condition-case err (all-completions "" '("a") nil nil nil) (error (car err)))
(condition-case err (test-completion "" '("a") nil nil) (error (car err)))
(fboundp 'minibuffer-prompt)
(fboundp 'minibuffer-contents)
(fboundp 'exit-minibuffer)
(condition-case err (minibuffer-prompt nil) (error (car err)))
(condition-case err (minibuffer-contents nil) (error (car err)))
(condition-case err (exit-minibuffer nil) (error (car err)))
(condition-case err (abort-recursive-edit) (error (car err)))
(condition-case err (abort-recursive-edit nil) (error (car err)))
