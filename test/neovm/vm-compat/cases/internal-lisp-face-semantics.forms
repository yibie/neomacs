(fboundp 'internal-lisp-face-p)
(internal-lisp-face-p 'default)
(internal-lisp-face-p 'mode-line)
(internal-lisp-face-p 'unknown-face)
(internal-lisp-face-p nil)
(internal-lisp-face-p 1)
(internal-lisp-face-p "default")
(internal-lisp-face-p "unknown-face")
(condition-case err
    (internal-lisp-face-p 'default 1)
  (error err))
(condition-case err
    (internal-lisp-face-p 'default nil nil)
  (error err))

(fboundp 'internal-get-lisp-face-attribute)
(internal-get-lisp-face-attribute 'default :foreground)
(internal-get-lisp-face-attribute 'default :background)
(internal-get-lisp-face-attribute 'default :weight)
(internal-get-lisp-face-attribute 'default :slant)
(internal-get-lisp-face-attribute 'default :underline)
(internal-get-lisp-face-attribute 'default :height)
(internal-get-lisp-face-attribute 'default :family)
(internal-get-lisp-face-attribute 'default :width)
(internal-get-lisp-face-attribute 'mode-line :foreground)
(internal-get-lisp-face-attribute 'bold :weight)
(internal-get-lisp-face-attribute 'italic :slant)
(internal-get-lisp-face-attribute 'underline :underline)
(internal-get-lisp-face-attribute 'default :foreground t)
(condition-case err
    (internal-get-lisp-face-attribute 'default :foreground 1)
  (error err))
(condition-case err
    (internal-get-lisp-face-attribute 'unknown-face :foreground)
  (error err))
(condition-case err
    (internal-get-lisp-face-attribute 1 :foreground)
  (error err))
(condition-case err
    (internal-get-lisp-face-attribute nil :foreground)
  (error err))
(condition-case err
    (internal-get-lisp-face-attribute 'default 1)
  (error err))
(condition-case err
    (internal-get-lisp-face-attribute 'default 'foreground)
  (error err))
(condition-case err
    (internal-get-lisp-face-attribute 'default :bogus)
  (error err))
(condition-case err
    (internal-get-lisp-face-attribute 'default nil)
  (error err))
(condition-case err
    (internal-get-lisp-face-attribute 'default)
  (error err))
(condition-case err
    (internal-get-lisp-face-attribute 'default :foreground nil nil)
  (error err))
