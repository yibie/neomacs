; Lock in minibufferp arity/type behavior in batch mode.
(fboundp 'minibufferp)
(minibufferp)
(minibufferp nil)
(minibufferp (current-buffer))
(minibufferp "x")
(minibufferp nil nil)
(condition-case err (minibufferp 1) (error err))
(condition-case err (minibufferp t) (error err))
(condition-case err (minibufferp nil nil nil) (error err))
(condition-case err (minibufferp "x" nil nil nil) (error err))
