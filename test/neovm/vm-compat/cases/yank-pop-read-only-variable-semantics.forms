; yank-pop should honor dynamic/local/global `buffer-read-only`.
(with-temp-buffer
  (kill-new "a")
  (kill-new "b")
  (yank)
  (setq last-command 'yank)
  (setq buffer-read-only t)
  (condition-case err (yank-pop 1) (error (car err))))

(with-temp-buffer
  (kill-new "a")
  (kill-new "b")
  (yank)
  (setq last-command 'yank)
  (let ((buffer-read-only t))
    (condition-case err (yank-pop 1) (error (car err)))))

(let ((orig (default-value 'buffer-read-only)))
  (unwind-protect
      (progn
        (setq-default buffer-read-only t)
        (with-temp-buffer
          (kill-new "a")
          (kill-new "b")
          (yank)
          (setq last-command 'yank)
          (condition-case err
              (progn
                (yank-pop 1)
                (buffer-string))
            (error (car err)))))
    (setq-default buffer-read-only orig)))

(with-temp-buffer
  (kill-new "a")
  (kill-new "b")
  (yank)
  (setq last-command 'yank)
  (yank-pop 1)
  (buffer-string))
