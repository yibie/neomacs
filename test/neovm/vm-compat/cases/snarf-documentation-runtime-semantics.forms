(Snarf-documentation "DOC")
(condition-case err (Snarf-documentation "") (error (car err)))
(condition-case err (Snarf-documentation "./") (error (car err)))
(condition-case err (Snarf-documentation "NO_SUCH_DOC_FILE") (error (car err)))
(condition-case err (Snarf-documentation "/tmp/no-such-doc") (error (car err)))
(condition-case err (Snarf-documentation nil) (error err))
(condition-case err (Snarf-documentation 1) (error err))
(condition-case err (Snarf-documentation '(a)) (error err))
(condition-case err (Snarf-documentation "" nil) (error err))
