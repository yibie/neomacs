(fboundp 'kmacro-add-counter)
(condition-case err (kmacro-add-counter 1) (error err))
(fboundp 'kmacro-p)
(condition-case err (kmacro-p nil) (error err))
(fboundp 'kmacro-set-counter)
(condition-case err (kmacro-set-counter 1) (error err))
(fboundp 'kmacro-set-format)
(condition-case err (kmacro-set-format "%d") (error err))
(fboundp 'last-kbd-macro)
(condition-case err (last-kbd-macro) (error err))
(fboundp 'defining-kbd-macro-p)
(condition-case err (defining-kbd-macro-p) (error err))
(fboundp 'executing-kbd-macro-p)
(condition-case err (executing-kbd-macro-p) (error err))
