(fboundp 'terminal-parameter)
(fboundp 'set-terminal-parameter)

(let* ((term (frame-terminal))
       (_ (set-terminal-parameter term "neovm-vmcompat-edge-key" 1)))
  (condition-case err
      (terminal-parameter term "neovm-vmcompat-edge-key")
    (wrong-type-argument (car err))))

(let* ((term (frame-terminal))
       (_ (set-terminal-parameter term "neovm-vmcompat-edge-key" 1))
       (_ (set-terminal-parameter term 'neovm-vmcompat-edge-symbol 7)))
  (terminal-parameter term 'neovm-vmcompat-edge-symbol))

(let* ((term (frame-terminal))
       (_ (set-terminal-parameter term 'neovm-vmcompat-edge-symbol 3))
       (_ (set-terminal-parameter term 'neovm-vmcompat-edge-symbol 4)))
  (terminal-parameter term 'neovm-vmcompat-edge-symbol))

(condition-case err
    (set-terminal-parameter (frame-terminal) 7 9)
  (wrong-type-argument (car err)))
