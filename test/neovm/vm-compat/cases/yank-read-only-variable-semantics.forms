; yank should honor dynamic/local/global `buffer-read-only`.
(with-temp-buffer
  (kill-new "x")
  (setq buffer-read-only t)
  (condition-case err (yank) (error (car err))))
(with-temp-buffer
  (kill-new "x")
  (let ((buffer-read-only t))
    (condition-case err (yank) (error (car err)))))
(let ((orig (default-value 'buffer-read-only)))
  (unwind-protect
      (progn
        (setq-default buffer-read-only t)
        (with-temp-buffer
          (kill-new "x")
          (condition-case err (yank) (error (car err)))))
    (setq-default buffer-read-only orig)))
(with-temp-buffer
  (kill-new "x")
  (yank)
  (buffer-string))
