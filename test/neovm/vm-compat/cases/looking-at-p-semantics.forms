(fboundp 'looking-at-p)
(with-temp-buffer
  (insert "hello")
  (goto-char (point-min))
  (looking-at-p "hel"))
(with-temp-buffer
  (insert "hello")
  (goto-char (point-min))
  (looking-at-p "world"))
(with-temp-buffer
  (insert "hello")
  (goto-char (point-min))
  (set-match-data (list 7 8))
  (looking-at-p "hel")
  (match-data))
(with-temp-buffer
  (insert "hello")
  (goto-char (point-min))
  (set-match-data nil)
  (looking-at "h\\(e\\)l")
  (match-beginning 1))
(condition-case err (looking-at-p "[") (invalid-regexp (car err)))
(condition-case err (looking-at-p "x" nil) (wrong-number-of-arguments (car err)))
