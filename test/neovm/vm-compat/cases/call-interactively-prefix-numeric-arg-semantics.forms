; call-interactively should use current-prefix-arg numeric conversion.

(with-temp-buffer
  (insert "abcdef")
  (goto-char 1)
  (let ((current-prefix-arg 3))
    (call-interactively 'forward-char)
    (point)))

(with-temp-buffer
  (insert "abcdef")
  (goto-char 1)
  (let ((current-prefix-arg 3))
    (command-execute 'forward-char)
    (point)))

(with-temp-buffer
  (insert "abcdef")
  (goto-char 1)
  (let ((current-prefix-arg 3))
    (call-interactively 'delete-char)
    (buffer-string)))

(with-temp-buffer
  (insert "abcdef")
  (goto-char 1)
  (let ((current-prefix-arg 3))
    (command-execute 'delete-char)
    (buffer-string)))

(with-temp-buffer
  (insert "abcdef")
  (goto-char 1)
  (let ((current-prefix-arg '(4)))
    (call-interactively 'forward-char)
    (point)))

(with-temp-buffer
  (insert "abcdef")
  (goto-char 4)
  (let ((current-prefix-arg '-))
    (call-interactively 'backward-char)
    (point)))

(with-temp-buffer
  (insert "abcdef")
  (goto-char 1)
  (let ((current-prefix-arg '(foo)))
    (call-interactively 'forward-char)
    (point)))
