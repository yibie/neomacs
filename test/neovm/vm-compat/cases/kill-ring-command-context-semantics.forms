; kill-new / kill-append / current-kill command-context semantics.
(condition-case err
    (current-kill 0 t)
  (error (car err)))

(kill-new "a")
(current-kill 0 t)

(kill-new "b")
(list
 (current-kill 0 t)
 (current-kill 1 t)
 (current-kill -1 t)
 (current-kill 0 t))

(list
 (current-kill 0 nil)
 (current-kill 1 nil)
 (current-kill 0 t))

(kill-new "base")
(kill-append "-tail" nil)
(current-kill 0 t)

(kill-new "base")
(kill-append "head-" t)
(current-kill 0 t)

(kill-new "first")
(kill-new "second" t)
(current-kill 0 t)

(kill-new "")
(current-kill 0 t)
