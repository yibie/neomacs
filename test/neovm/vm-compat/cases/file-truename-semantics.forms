; file-truename path normalization and internal COUNTER semantics.

(file-truename "/tmp/neovm-file-truename-missing")

(let ((default-directory "/tmp/"))
  (file-truename "neovm-file-truename-rel"))

(file-truename "/tmp/../tmp/")

(condition-case err
    (file-truename 1)
  (error err))

(condition-case err
    (file-truename "/tmp" 1)
  (error err))

(file-truename "/tmp" '(visited))

(file-truename "/tmp" '(visited) 1)

(condition-case err
    (file-truename "/tmp" nil nil nil)
  (error err))
