; command-execute/call-interactively should dispatch line-motion commands with Emacs-style boundary signals.
(condition-case err
    (with-temp-buffer
      (insert "abc")
      (goto-char 1)
      (command-execute 'forward-char)
      (point))
  (error err))

(condition-case err
    (with-temp-buffer
      (insert "abc")
      (goto-char 1)
      (command-execute 'next-line)
      (point))
  (error err))

(condition-case err
    (with-temp-buffer
      (insert "abc")
      (goto-char 2)
      (command-execute 'previous-line)
      (point))
  (error err))

(condition-case err
    (with-temp-buffer
      (insert "abc")
      (goto-char 1)
      (call-interactively 'next-line)
      (point))
  (error err))

(condition-case err
    (with-temp-buffer
      (insert "abc")
      (goto-char 2)
      (call-interactively 'previous-line)
      (point))
  (error err))

(condition-case err
    (with-temp-buffer
      (insert "abc")
      (goto-char 2)
      (command-execute 'beginning-of-line)
      (point))
  (error err))

(condition-case err
    (with-temp-buffer
      (insert "abc")
      (goto-char 1)
      (command-execute 'end-of-line)
      (point))
  (error err))

(condition-case err
    (command-execute 'other-window)
  (error err))
