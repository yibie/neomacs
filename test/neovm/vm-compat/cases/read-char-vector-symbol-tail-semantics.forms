(let ((unread-command-events (list [foo] 97)))
  (progn
    (read-char nil nil 0)
    unread-command-events))

(let ((unread-command-events (list [foo] 98 'bar)))
  (progn
    (read-char nil nil 0)
    unread-command-events))

(let ((unread-command-events '([foo] . 97)))
  (condition-case err
      (progn
        (read-char nil nil 0)
        unread-command-events)
    (error (list (car err) unread-command-events))))
