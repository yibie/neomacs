; Syntax table runtime/value semantics.

(fboundp 'make-syntax-table)
(fboundp 'standard-syntax-table)
(fboundp 'syntax-table)

(char-table-p (make-syntax-table))
(eq (char-table-subtype (make-syntax-table)) 'syntax-table)

(let* ((parent (make-syntax-table))
       (child (make-syntax-table parent)))
  (char-table-p (char-table-parent child)))

(let* ((parent (make-syntax-table))
       (child (make-syntax-table parent)))
  (eq (char-table-subtype child) 'syntax-table))

(char-table-p (standard-syntax-table))
(eq (char-table-subtype (standard-syntax-table)) 'syntax-table)

(with-temp-buffer
  (char-table-p (syntax-table)))

(with-temp-buffer
  (eq (char-table-subtype (syntax-table)) 'syntax-table))

(condition-case err (make-syntax-table 1) (error (car err)))
(condition-case err (standard-syntax-table 1) (wrong-number-of-arguments (car err)))
(condition-case err (syntax-table 1) (wrong-number-of-arguments (car err)))
