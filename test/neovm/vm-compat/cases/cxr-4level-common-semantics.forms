; Common 4-level list accessor semantics.

(cadddr '(a b c d e))
(cddddr '(a b c d e))

(cadddr '(a b c))
(cddddr '(a b c d))

(list (cadddr nil) (cddddr nil))

(condition-case err
    (cadddr '(1 . 2))
  (error (list (car err) (car (cdr err)))))

(condition-case err
    (cddddr '(1 . 2))
  (error (list (car err) (car (cdr err)))))
