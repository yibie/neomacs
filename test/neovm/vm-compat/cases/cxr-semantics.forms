; Core 2-level list accessor semantics.

(caar '((1 . 2) (3 . 4)))
(cadr '(1 2 3))
(cdar '((1 . 2) (3 . 4)))
(cddr '(1 2 3))

(list (caar nil) (cadr nil) (cdar nil) (cddr nil))

(cadr '((a b) c d))

(condition-case err
    (caar '(1 . 2))
  (error (list (car err) (car (cdr err)))))

(condition-case err
    (cadr '(1 . 2))
  (error (list (car err) (car (cdr err)))))

(condition-case err
    (cdar '(1 . 2))
  (error (list (car err) (car (cdr err)))))

(condition-case err
    (cddr '(1 . 2))
  (error (list (car err) (car (cdr err)))))

(condition-case err
    (cadr 1)
  (error (list (car err) (car (cdr err)))))
