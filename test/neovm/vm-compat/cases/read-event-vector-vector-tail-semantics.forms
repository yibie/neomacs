(let ((unread-command-events (list [foo] [bar])))
  (condition-case err
      (progn
        (read-event nil nil 0)
        unread-command-events)
    (error (list (car err) unread-command-events))))

(let ((unread-command-events '([foo] . [bar])))
  (condition-case err
      (progn
        (read-event nil nil 0)
        unread-command-events)
    (error (list (car err) unread-command-events))))

(let ((unread-command-events (list [foo] 97 [bar])))
  (condition-case err
      (progn
        (read-event nil nil 0)
        unread-command-events)
    (error (list (car err) unread-command-events))))
