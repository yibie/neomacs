; transpose-sexps should honor dynamic/local/global `buffer-read-only`.
(with-temp-buffer
  (insert "(aa) (bb)")
  (goto-char 5)
  (setq buffer-read-only t)
  (condition-case err (transpose-sexps 1) (error (car err))))

(with-temp-buffer
  (insert "(aa) (bb)")
  (goto-char 5)
  (let ((buffer-read-only t))
    (condition-case err (transpose-sexps 1) (error (car err)))))

(let ((orig (default-value 'buffer-read-only)))
  (unwind-protect
      (progn
        (setq-default buffer-read-only t)
        (with-temp-buffer
          (insert "(aa) (bb)")
          (goto-char 5)
          (condition-case err
              (progn
                (transpose-sexps 1)
                (buffer-string))
            (error (car err)))))
    (setq-default buffer-read-only orig)))

(with-temp-buffer
  (insert "(aa) (bb)")
  (goto-char 5)
  (transpose-sexps 1)
  (buffer-string))
