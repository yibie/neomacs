; Lock in runtime identity helper arity and return shapes.
(fboundp 'system-name)
(fboundp 'emacs-version)
(fboundp 'emacs-pid)

(stringp (system-name))
(stringp (emacs-version))
(integerp (emacs-pid))
(> (emacs-pid) 0)

(condition-case err (system-name nil) (error (car err)))
(condition-case err (stringp (emacs-version nil)) (error (car err)))
(condition-case err (emacs-pid nil) (error (car err)))
