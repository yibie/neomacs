(fboundp 'substitute-command-keys)
(condition-case err (substitute-command-keys) (error (car err)))
(condition-case err (substitute-command-keys "" nil) (error (car err)))

(fboundp 'Snarf-documentation)
(condition-case err (Snarf-documentation) (error (car err)))
(condition-case err (Snarf-documentation "" nil) (error (car err)))

(fboundp 'documentation)
(condition-case err (documentation 'car nil nil) (error (car err)))

(fboundp 'documentation-property)
(condition-case err (documentation-property 'x 'y nil nil) (error (car err)))

(fboundp 'help-function-arglist)
(condition-case err (help-function-arglist 'car nil nil) (error (car err)))

(fboundp 'describe-function)
(condition-case err (describe-function) (error (car err)))
(condition-case err (describe-function 'car nil) (error (car err)))

(fboundp 'describe-variable)
(condition-case err (describe-variable) (error (car err)))
(condition-case err (describe-variable 'fill-column nil) (error (car err)))
