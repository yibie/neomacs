; filename builtin type strictness and expand-file-name default-dir fallback.
(condition-case err (file-name-directory :x) (error (car err)))
(condition-case err (file-name-nondirectory :x) (error (car err)))
(condition-case err (file-name-extension :x) (error (car err)))
(condition-case err (file-name-sans-extension :x) (error (car err)))
(condition-case err (file-name-as-directory :x) (error (car err)))
(condition-case err (directory-file-name :x) (error (car err)))
(condition-case err (file-exists-p nil) (error (car err)))
(condition-case err (file-readable-p nil) (error (car err)))
(condition-case err (file-writable-p nil) (error (car err)))
(condition-case err (file-directory-p nil) (error (car err)))
(condition-case err (file-regular-p nil) (error (car err)))
(condition-case err (file-symlink-p nil) (error (car err)))
(condition-case err (expand-file-name nil) (error (car err)))
(expand-file-name "a" :x)
(expand-file-name "a" t)
(condition-case err (expand-file-name "a" nil nil) (error (car err)))
(file-name-extension "foo.txt" t)
(file-name-extension "foo" t)
(file-name-extension ".bashrc" t)
(file-name-extension "a." t)
(condition-case err (file-name-extension "x" nil nil) (error (car err)))
(file-name-sans-extension "foo.bar/")
(file-name-sans-extension "foo/")
(file-name-sans-extension "/tmp/foo.bar/")
