; transpose-sexps should move the preceding sexp across N sexps with Emacs point semantics.
(condition-case err
    (with-temp-buffer
      (insert "(a) (b) (c)")
      (goto-char 4)
      (transpose-sexps 1)
      (list (buffer-string) (point)))
  (error err))

(condition-case err
    (with-temp-buffer
      (insert "(a) (b) (c)")
      (goto-char 4)
      (transpose-sexps 2)
      (list (buffer-string) (point)))
  (error err))

(condition-case err
    (with-temp-buffer
      (insert "(a) (b) (c)")
      (goto-char 1)
      (transpose-sexps 2)
      (list (buffer-string) (point)))
  (error err))

(condition-case err
    (with-temp-buffer
      (insert "(a) (b) (c)")
      (goto-char 2)
      (transpose-sexps 1)
      err)
  (error err))

(condition-case err
    (with-temp-buffer
      (insert "(a) (b) (c)")
      (goto-char 5)
      (transpose-sexps -1))
  (error err))

(condition-case err
    (with-temp-buffer
      (insert "(a) (b) (c)")
      (goto-char (point-max))
      (transpose-sexps -1)
      (list (buffer-string) (point)))
  (error err))
