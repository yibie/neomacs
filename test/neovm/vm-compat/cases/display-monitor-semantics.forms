; Monitor attribute builtins should expose batch-style shape and error class.
(length (display-monitor-attributes-list))
(let* ((attrs (car (display-monitor-attributes-list)))
       (g (cdr (assq 'geometry attrs)))
       (w (cdr (assq 'workarea attrs)))
       (mm (cdr (assq 'mm-size attrs)))
       (frames (cdr (assq 'frames attrs))))
  (list g w mm (listp frames)))
(let* ((attrs (frame-monitor-attributes))
       (g (cdr (assq 'geometry attrs)))
       (w (cdr (assq 'workarea attrs)))
       (mm (cdr (assq 'mm-size attrs)))
       (frames (cdr (assq 'frames attrs))))
  (list g w mm (listp frames)))
(equal (display-monitor-attributes-list) (display-monitor-attributes-list nil))
(equal (frame-monitor-attributes) (frame-monitor-attributes nil))
(let ((term (car (terminal-list))))
  (length (display-monitor-attributes-list term)))
(let ((f (selected-frame)))
  (length (display-monitor-attributes-list f)))
(let* ((attrs (frame-monitor-attributes (selected-frame)))
       (frames (cdr (assq 'frames attrs))))
  (and (= (length frames) 1)
       (frame-live-p (car frames))))
(condition-case err
    (display-monitor-attributes-list 999999)
  (error (car err)))
(condition-case err
    (frame-monitor-attributes 999999)
  (error (car err)))
(let* ((attrs (frame-monitor-attributes (car (terminal-list))))
       (g (cdr (assq 'geometry attrs))))
  g)
