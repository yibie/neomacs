; call-interactively prefix args should preserve line-motion boundary signals.

(with-temp-buffer
  (insert "abcdef")
  (goto-char 1)
  (let ((current-prefix-arg '(16)))
    (condition-case err
        (call-interactively 'forward-char)
      (error (car err)))))

(with-temp-buffer
  (insert "abcdef")
  (goto-char 1)
  (let ((current-prefix-arg '(16)))
    (condition-case err
        (command-execute 'forward-char)
      (error (car err)))))

(with-temp-buffer
  (insert "abcdef")
  (goto-char 1)
  (let ((current-prefix-arg '-))
    (condition-case err
        (call-interactively 'backward-char)
      (error (car err)))))

(with-temp-buffer
  (insert "abcdef")
  (goto-char 1)
  (let ((current-prefix-arg '-))
    (condition-case err
        (command-execute 'backward-char)
      (error (car err)))))
