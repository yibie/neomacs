; command-execute/call-interactively parity for commandp errors and symbol-designator resolution.
(condition-case err (command-execute 'ignore) (error err))
(condition-case err (command-execute 'car) (error err))
(condition-case err (command-execute 'nonexistent) (error err))
(condition-case err (call-interactively 'ignore) (error err))
(condition-case err (call-interactively 'car) (error err))
(condition-case err (call-interactively 'nonexistent) (error err))

(let ((orig (symbol-function 't)))
  (unwind-protect
      (progn
        (fset t 'ignore)
        (condition-case err (command-execute t) (error err)))
    (fset 't orig)))

(let ((orig-k (symbol-function :vm-cmd-run-k)))
  (unwind-protect
      (progn
        (fset :vm-cmd-run-k 'ignore)
        (condition-case err (command-execute :vm-cmd-run-k) (error err)))
    (fset :vm-cmd-run-k orig-k)))
