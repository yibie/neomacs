; Lock in batch reader semantics for path/buffer/command/variable prompts.
(fboundp 'read-file-name)
(condition-case err (read-file-name "") (error err))
(condition-case err (read-file-name "" nil nil nil nil nil nil) (error err))
(condition-case err (read-file-name 1) (error err))
(condition-case err (read-file-name "" 1) (error err))
(condition-case err (read-file-name "" nil 1) (error err))
(condition-case err (read-file-name "" nil nil nil 1) (error err))
(condition-case err (read-file-name "" nil nil nil nil nil) (error err))
(condition-case err (read-file-name "" nil nil nil nil 1) (error err))
(condition-case err (read-file-name "" nil nil nil nil "x") (error err))

(fboundp 'read-directory-name)
(condition-case err (read-directory-name "") (error err))
(condition-case err (read-directory-name "" nil nil nil nil nil) (error err))
(condition-case err (read-directory-name 1) (error err))
(condition-case err (read-directory-name "" 1) (error err))
(condition-case err (read-directory-name "" nil 1) (error err))
(condition-case err (read-directory-name "" nil nil nil 1) (error err))
(condition-case err (read-directory-name "" nil nil nil nil) (error err))
(condition-case err (read-directory-name "" nil nil nil nil 1) (error err))

(fboundp 'read-buffer)
(condition-case err (read-buffer "") (error err))
(condition-case err (read-buffer "" nil nil nil nil) (error err))
(condition-case err (read-buffer 1) (error err))
(condition-case err (read-buffer "" 1) (error err))
(condition-case err (read-buffer "" nil nil nil) (error err))
(condition-case err (read-buffer "" nil nil nil [1]) (error err))

(fboundp 'read-command)
(condition-case err (read-command "") (error err))
(condition-case err (read-command "" nil nil) (error err))
(condition-case err (read-command 1) (error err))
(condition-case err (read-command "" 1) (error err))
(condition-case err (read-command "" nil) (error err))
(condition-case err (read-command "" [1]) (error err))

(fboundp 'read-variable)
(condition-case err (read-variable "") (error err))
(condition-case err (read-variable "" nil nil) (error err))
(condition-case err (read-variable 1) (error err))
(condition-case err (read-variable "" 1) (error err))
(condition-case err (read-variable "" nil) (error err))
(condition-case err (read-variable "" [1]) (error err))

(condition-case err (read-file-name "" nil nil nil nil nil nil nil) (error err))
(condition-case err (read-directory-name "" nil nil nil nil nil nil) (error err))
(condition-case err (read-buffer "" nil nil nil nil nil) (error err))
(condition-case err (read-command "" nil nil nil) (error err))
(condition-case err (read-variable "" nil nil nil) (error err))
