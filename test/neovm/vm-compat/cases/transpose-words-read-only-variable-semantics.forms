; transpose-words should honor dynamic/local/global `buffer-read-only`.
(with-temp-buffer
  (insert "aa bb")
  (goto-char 1)
  (setq buffer-read-only t)
  (condition-case err (transpose-words 1) (error (car err))))

(with-temp-buffer
  (insert "aa bb")
  (goto-char 1)
  (let ((buffer-read-only t))
    (condition-case err (transpose-words 1) (error (car err)))))

(let ((orig (default-value 'buffer-read-only)))
  (unwind-protect
      (progn
        (setq-default buffer-read-only t)
        (with-temp-buffer
          (insert "aa bb")
          (goto-char 1)
          (condition-case err
              (progn
                (transpose-words 1)
                (buffer-string))
            (error (car err)))))
    (setq-default buffer-read-only orig)))

(with-temp-buffer
  (insert "aa bb")
  (goto-char 1)
  (transpose-words 1)
  (buffer-string))
